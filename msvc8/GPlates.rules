<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Qt Build Tools"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="Building UI Files"
			DisplayName="Build UI Files"
			CommandLine="uic &quot;$(InputDir)/$(InputName).ui&quot; -o  &quot;$(InputDir)/$(InputName).h&quot;"
			Outputs="$(InputDir)/$(InputName).h"
			FileExtensions="*.ui"
			ExecutionDescription="Executing UIC on $(InputName)"
			BatchingSeparator=""
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="Build MOC Files"
			DisplayName="Building MOC Files"
			CommandLine="moc &quot;$(InputDir)/$(InputName).h&quot; -o &quot;$(InputDir)/moc_$(InputName).cc&quot;"
			Outputs="$(InputDir)/moc_$(InputName).cc"
			FileExtensions="*.h"
			ExecutionDescription="Executing MOC on $(InputName)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="Building QRC Files"
			DisplayName="Build QRC Files"
			CommandLine="rcc &quot;$(InputDir)/$(InputName).qrc&quot; -o  &quot;$(InputDir)/qrc_$(InputName).cc&quot;"
			Outputs="$(InputDir)/qrc_$(InputName).cc"
			FileExtensions="*.qrc"
			ExecutionDescription="Executing RCC on $(InputName)"
			BatchingSeparator=""			
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
