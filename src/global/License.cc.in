// Using 'global/' prefix since the generated version of this file might not be in same directory as "License.h" (ie, in 'global/').
#include "global/License.h"

// If you see \@GPLATES_COPYRIGHT_STRING\@ in this string then you are looking at the
// file that the CMake build system will use to generate a new file with the appropriate
// GPlates version string replacement.
// If not then you are looking at the file generated by CMake.
QString
GPlatesGlobal::License::get_copyright_string()
{
    // Interpret as raw string literal since can be multi-line.
    return QString(R"(@GPLATES_COPYRIGHT_STRING@)");
}

QString
GPlatesGlobal::License::get_html_copyright_string()
{
    // Interpret as raw string literal since can be multi-line.
    return QString(R"(@GPLATES_HTML_COPYRIGHT_STRING@)");
}
