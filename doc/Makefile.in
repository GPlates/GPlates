@SET_MAKE@

subdir = doc
prefix = @prefix@

FIGURES = fig_small_circle.eps

DISTFILES = Makefile.in doxygen.conf.in $(FIGURES)

INSTALL = @INSTALL@

all:

doc:	doxygen.conf $(FIGURES:.eps=.png)
	doxygen $<

install:

clean:
	rm -rf html
	rm $(FIGURES:.eps=.png)

%.png : %.eps
	convert -quality 100 $< $@

dep:

distdir = ../@PACKAGE_TARNAME@-@PACKAGE_VERSION@/$(subdir)
dist:	Makefile $(DISTFILES)
	for file in $(DISTFILES) ; do \
		ln $$file $(distdir) 2>/dev/null || \
			cp -p $$file $(distdir); \
	done
